{
  "name": "gauge-taiko-template",
  "description": "Starter template for writing JavaScript tests for Gauge",
  "scripts": {
    "local": "gauge run specs --env local -v -p -n=3 --tags 'next | BedManagement'",
    "next": "gauge run specs --env next -v -p -n=3 --tags 'next'",
    "log4j": "gauge run specs --env qa08 -v -p -n=3 --tags 'smoke | BedManagement'",
    "test": "gauge run specs --env next -v -p -n=3 --tags 'next | BedManagement'"
  },
  "dependencies": {
    "taiko": "latest",
    "axios": "latest",
    "buffer": "latest",
    "uuid": "latest"
  }
}
