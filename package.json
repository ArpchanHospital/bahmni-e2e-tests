{
  "name": "gauge-taiko-template",
  "description": "Starter template for writing JavaScript tests for Gauge",
  "scripts": {
    "local": "gauge run specs --env local -v -p --tags 'clinic | BedManagement'",
    "covid-19": "gauge run specs --env clinics-bahmni-covid19 -v -p -n=3 --tags 'Demo'",
    "lablite": "gauge run specs --env next -v -p -n=3 --tags 'clinic'",
    "hospital": "gauge run specs --env demo0.9.3 -v -p -n=3 --tags 'hospital | BedManagement | IPModule'",
    "test": "gauge run specs --env next -v -p -n=3 --tags 'clinic | BedManagement'"
  },
  "dependencies": {
    "taiko": "^1.3.2",
    "axios": "latest",
    "buffer": "latest",
    "uuid": "latest",
    "json2csv": "latest",
    "csvtojson": "latest"
  }
}
